\auth.js",["169"],"import { REGISTER_FAIL, REGISTER_SUCCESS, USER_LOADED, AUTH_ERROR, LOGIN_SUCCESS, LOGIN_FAIL, LOGOUT, ACCOUNT_DELETED } from \"../actions/types\";\r\n\r\nconst intialState = {\r\n    token: localStorage.getItem(\"token\"),\r\n    isAuthenticated: null,\r\n    loading: true,\r\n    user: null\r\n}\r\n\r\nexport default function(state=intialState, action) {\r\n    \r\n    const { type, payload } = action;  \r\n    switch(type) {\r\n        case USER_LOADED:\r\n            return {\r\n                ...state,\r\n                isAuthenticated: true,\r\n                loading: false,\r\n                user: payload\r\n            };\r\n        case REGISTER_SUCCESS:\r\n        case LOGIN_SUCCESS:\r\n            localStorage.setItem(\"token\", payload.token);\r\n            return {\r\n                ...state,\r\n                ...payload,\r\n                isAuthenticated: true,\r\n                loading: false\r\n            };\r\n\r\n        case REGISTER_FAIL: \r\n        case AUTH_ERROR: \r\n        case LOGIN_FAIL:    \r\n        case LOGOUT:\r\n        case ACCOUNT_DELETED:\r\n            localStorage.removeItem(\"token\");\r\n            return {\r\n                ...state,\r\n                token: null,\r\n                isAuthenticated: false,\r\n                loading: false \r\n            };\r\n\r\n        default:\r\n            return state;\r\n    }\r\n}","T:\\thefyp\\fyp\\client\\src\\utils\\setAuthToken.js",[],"T:\\thefyp\\fyp\\client\\src\\components\\dashboard\\Dashboard.js",["170"],"T:\\thefyp\\fyp\\client\\src\\components\\routing\\PrivateRoute.js",["171","172"],"import React, { Component } from \"react\";\r\nimport { connect } from \"react-redux\";\r\nimport { Redirect, Route } from \"react-router-dom\";\r\nimport auth from \"../../reducers/auth\";\r\n\r\nconst PrivateRoute = ({ component: Component, auth, ...rest}) => (\r\n    <Route {...rest} render={props => !auth.isAuthenticated && !auth.loading ? <Redirect to=\"login\" /> : (<Component {...props}/>)}/>\r\n)\r\n\r\nconst mapStateToProps = state => ({\r\n    auth: state.auth,\r\n})\r\nexport default connect(mapStateToProps)(PrivateRoute);","T:\\thefyp\\fyp\\client\\src\\reducers\\profile.js",["173"],"T:\\thefyp\\fyp\\client\\src\\actions\\profile.js",[],"T:\\thefyp\\fyp\\client\\src\\components\\layout\\Spinner.js",["174"],"import React, { Fragment } from \"react\";\r\nimport spinner from \"../../img/45.gif\";\r\n\r\nexport default () => (\r\n    <Fragment>\r\n        <img\r\n            src={spinner}\r\n            style={{width: \"150px\", margin: \"auto\", display: \"block\"}}\r\n            alt=\"Loaidng\"\r\n        />\r\n    </Fragment>\r\n);","T:\\thefyp\\fyp\\client\\src\\components\\profileforms\\CreateProfile.js",[],"T:\\thefyp\\fyp\\client\\src\\components\\dashboard\\DashboardActions.js",[],"T:\\thefyp\\fyp\\client\\src\\components\\profileforms\\EditProfile.js",["175","176","177","178","179"],"import React, { Fragment, useEffect, useState } from \"react\";\r\nimport { connect } from \"react-redux\";\r\nimport { Link, withRouter } from \"react-router-dom\";\r\nimport { createProfile, getCurrentProfile } from \"../../actions/profile\";\r\n\r\nconst EditProfile = ({ profile: {profile, loading}, createProfile, history, getCurrentProfile }) => {\r\n\r\n    const [formData, setFromData] = useState({\r\n        company: \"\",\r\n        website: \"\",\r\n        location: \"\",\r\n        status: \"\",\r\n        skills: \"\",\r\n        bio: \"\",\r\n        twitter: \"\",\r\n        facebook: \"\",\r\n        instagram: \"\",\r\n    });\r\n\r\n    const {\r\n        company,\r\n        website,\r\n        location,\r\n        status,\r\n        skills,\r\n        bio,\r\n        twitter,\r\n        facebook,\r\n        instagram\r\n    } = formData;\r\n\r\n\r\n    const [displaySocials, setDisplaySocials] = useState(false); \r\n\r\n    useEffect(() => {\r\n        getCurrentProfile();\r\n\r\n        setFromData({\r\n 